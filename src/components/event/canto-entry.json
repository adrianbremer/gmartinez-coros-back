{
    "collectionName": "components_event_canto_entry",
    "info": {
        "displayName": "Canto Entry",
        "description": "A song with additional notes for events",
        "icon": "music",
        "mainField": "song_name"
    },
    "options": {},
    "attributes": {
        "song": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::song.song",
            "required": true,
            "displayName": "Song"
        },
        "song_name": {
            "type": "string",
            "maxLength": 255,
            "displayName": "Song Name",
            "description": "Auto-populated from selected song"
        },
        "notes": {
            "type": "string",
            "maxLength": 255,
            "displayName": "Notes",
            "description": "Additional notes for this song in this event"
        },
        "performance_order": {
            "type": "integer",
            "min": 1,
            "displayName": "Performance Order",
            "description": "Order in which this song should be performed"
        },
        "tiempo_de_celebracion": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::liturgical-section.liturgical-section",
            "displayName": "Tiempo de celebracion"
        }
    }
}
